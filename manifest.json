{
  "manifest_version": 3,
  "name": "PowerPaste",
  "version": "1.0.0",
  "description": "엑셀 테이블을 HTML로 변환하여 웹페이지에 붙여넣기 할 수 있는 확장 프로그램입니다.",
  "author": "김경호",
  "permissions": [
    "clipboardRead",
    "clipboardWrite",
    "activeTab",
    "scripting"
  ],
  "host_permissions": [
    "file:///*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "action": {
    "default_icon": {
      "16": "icons/icon16.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    },
    "default_title": "PowerPaste"
  },
  "key": ""
} 